{
    "name": "mono-page/packages",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/mono-page/packages",
    "type": "library",
    "require": {
        "php": "^7.4",
        "mono-page/contracts": "^v0.0.1@dev",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpunit/php-code-coverage": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Monopage\\Packages\\": "source"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "preferred-update": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v0.0.1-dev"
        }
    },
    "repositories": []
}
